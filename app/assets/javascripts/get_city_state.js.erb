function reverseGeocodeLatLng(geocoder, pos) {
    var latlng = { lat: parseFloat(pos.lat), lng: parseFloat(pos.lng) };
    geocoder.geocode({ 'location': latlng }, function(results, status) {
        if (status === 'OK') {
            handleAddresses(results, queryWikipedia);
        }
    });
}

